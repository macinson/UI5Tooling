<mvc:View
   controllerName="ui5.tooling.controller.InvoiceList"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns:l="sap.ui.layout">
   <List
      class="sapUiResponsiveMargin"
      width="auto"
      items="{invoice>/Invoices}">
      <items>
         <ObjectListItem
            title="{invoice>ProductName}"
            type="Navigation"
            press=".fetchDetails"/>
      </items>
   </List>
</mvc:View>