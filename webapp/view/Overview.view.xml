<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="ui5.tooling.controller.Overview">
    <Page title="{i18n>homePageTitle}">
        <customHeader>
            <Toolbar>
                <ToolbarSpacer/>
                <Button text="{i18n>addInvoiceTitle}"
                    press=".showAddInvoice"/>
            </Toolbar>
        </customHeader>
        <content>
            <mvc:XMLView viewName="ui5.tooling.view.InvoiceList"/>
        </content>
    </Page>
</mvc:View>